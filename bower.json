{
  "name": "omh-web-visualizations",
  "homepage": "http://www.openmhealth.org",
  "authors": [ "Jasper Speicher <jasper@openmhealth.org>" ],
  "description": "A JS library for visualizing Open mHealth formatted data. Defaults for weight, heart rate, blood pressure, and activity are included.",
  "license": "Apache-2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/openmhealth/web-visualizations.git"
  },
  "keywords": [
    "mHealth",
    "mobile",
    "health",
    "web",
    "visualization",
    "charts"
  ],
  "main": [
    "./dist/omh-web-visualizations-all.js",
    "./dist/omh-web-visualizations-all.css"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "package.json",
    "demo",
    "gulpfile.js"
  ],
  "dependencies": {
    "d3": "^3.5.5",
    "plottable": "^2.0.0",
    "d3-tip": "~0.6.7",
    "moment": "~2.10.3"
  }
}
